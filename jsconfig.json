{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ESNext",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "cypress/**/*.js",
    "node_modules/**/*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
